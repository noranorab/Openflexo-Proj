<?xml version="1.0"?>
<!DOCTYPE XML>
<CONFIGURATION xmlns="http://mem4csd.telecom-paris.fr/heptaneExtract">

<TARGET NAME="MIPS" ENDIANNESS="BIG"/>

<!-- Compiler toolchain -->
<!-- ****************** -->

<!-- Preprocessor -->
<PREPROCESSOR NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-cpp" OPT="-DMIPS_ARCHI  -I. -I.."/>
<!-- Compiler (options -c -S are automatically put to generate assembly code) -->
<!-- Option -ggdb should be used if html output of critical path is asked for in subsequent WCET estimation -->
<COMPILER NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-gcc" OPT="-O0 -fomit-frame-pointer -ggdb"/>
<!-- Assembler -->
<ASSEMBLER NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-as" OPT=""/>
<!-- Linker (option -T script is automatically set when SCRIPT is specified) -->
<LINKER NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-ld" OPT="" SCRIPT=""/>
<!-- Objdump -->
<OBJDUMP NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-objdump" OPT=" -ztd "/>
<!-- Readelf (called with option -S) -->
<READELF NAME="/home/bob/Tools/heptane-master/CROSS_COMPILERS/MIPS/bin/mips-readelf" OPT=""/>


<!-- Directories of inputs, temporaries and outputs (default values . /tmp and .) -->
<!-- **************************************************************************** -->
<INPUTDIR NAME="."/>
<TMPDIR NAME="/tmp"/>
<RESULTDIR NAME="_RESULTDIR_"/>

<!-- Program parameters -->
<!-- ****************** -->

<!-- NAME: program name, used as a file name prefix when generating outputs in RESULTDIR -->
<!-- SOURCEFILE NAME: source file name (merged with INPUTDIR to load file) -->
<!-- SOURCEFILE TYPE: C/ASM/BINARY -->
<!-- ANNOTATIONFILE: input file defining the loop bounds, required for BINARY or ASM files only. Example: ANNOTATIONFILE="annot.xml" ).-->
<!-- Several C files are accepted, but not for BINARY/ASM types. -->

<PROGRAM NAME = "X_BENCH" ENTRYPOINT = "_ENTRY_POINT_" >
  <SOURCEFILE NAME="X_BENCH.c" TYPE="C" />
</PROGRAM>

<!-- OTHER OPTIONS -->
<STATISTICS VALUE="NO"/>

</CONFIGURATION>
